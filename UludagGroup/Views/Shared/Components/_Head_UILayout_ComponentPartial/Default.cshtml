@model HeadViewModel
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    @if (Model.faviconModel != null && !string.IsNullOrEmpty(Model.faviconModel.ImageUrl))
    {
        <link href="@Url.Content("~/Photos/Favicons/"+Model.faviconModel.ImageUrl)" rel="icon">
        <title>@Model.faviconModel.Title</title>
    }
    else
    {
        <link href="@Url.Content("~/img/favicon.ico")" rel="icon">
        <title>Uludağ Group</title>
    }

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Playfair+Display:wght@700;900&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="@Url.Content("~/lib/animate/animate.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/lib/owlcarousel/assets/owl.carousel.min.css")" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="@Url.Content("~/css/style.min.css")" rel="stylesheet" asp-append-version="true">
    <link href="@Url.Content("~/css/zero.min.css")" rel="stylesheet" asp-append-version="true">
</head>